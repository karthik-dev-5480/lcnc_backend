package com.lcncbe.service;

import org.jspecify.annotations.Nullable;

import com.lcncbe.dto.DataColumnRequest;

public interface DataColumnService {

	Object addColumn(Long tableId, DataColumnRequest request);

	@Nullable
	Object getColumnsByTable(Long tableId);

	@Nullable
	Object updateColumn(Long columnId, DataColumnRequest request);

	void deleteColumn(Long columnId);

}
